{
  "_version": "1.76.0",
  "sap.app": {
    "id": "membersapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.18.7",
      "toolsId": "3fbf5d7d-f297-4ca6-8d80-76630e0dc3a4"
    },
    "dataSources": {
      "mainService": {
        "uri": "/members/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "members-display": {
          "semanticObject": "members",
          "action": "display",
          "title": "{{members-display.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "membersapp.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {
        "flexibleColumnLayout": {
          "defaultTwoColumnLayoutType": "TwoColumnsBeginExpanded",
          "defaultThreeColumnLayoutType": "ThreeColumnsEndExpanded"
        },
        "routerClass": "sap.f.routing.Router"
      },
      "routes": [
        {
          "pattern": ":?query:",
          "name": "MembersList",
          "target": [
            "MembersList"
          ]
        },
        {
          "pattern": "Members({key}):?query:",
          "name": "MembersObjectPage",
          "target": [
            "MembersList",
            "MembersObjectPage"
          ]
        },
        {
          "name": "Members_loansObjectPage",
          "pattern": "Members({key})/loans({loansKey}):?query:",
          "target": [
            "MembersList",
            "MembersObjectPage",
            "Members_loansObjectPage"
          ]
        }
      ],
      "targets": {
        "MembersList": {
          "type": "Component",
          "id": "MembersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Members",
              "variantManagement": "Page",
              "navigation": {
                "Members": {
                  "detail": {
                    "route": "MembersObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "actions": {
                    "DataFieldForAction::MemberService.EntityContainer::createMember": {
                      "enableOnSelect": "single"
                    }
                  }
                }
              }
            }
          },
          "controlAggregation": "beginColumnPages",
          "contextPattern": ""
        },
        "MembersObjectPage": {
          "type": "Component",
          "id": "MembersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Members",
              "sectionLayout": "Page",
              "navigation": {
                "loans": {
                  "detail": {
                    "route": "Members_loansObjectPage"
                  }
                }
              }
            }
          },
          "controlAggregation": "midColumnPages",
          "contextPattern": "/Members({key})"
        },
        "Members_loansObjectPage": {
          "type": "Component",
          "id": "Members_loansObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "navigation": {},
              "contextPath": "/Members/loans"
            }
          },
          "controlAggregation": "endColumnPages",
          "contextPattern": "/Members({key})/loans({loansKey})"
        }
      }
    },
    "rootView": {
      "viewName": "sap.fe.templates.RootContainer.view.Fcl",
      "type": "XML",
      "async": true,
      "id": "appRootView"
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
